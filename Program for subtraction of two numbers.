#include <iostream> 

using namespace std;  

int main() 
{  
    int a, b, c;  
    cout << "Enter First Number: ";  
    cin >> a;  
    cout << "Enter Second Number: ";  
    cin >> b;  
    c = a - b;  
    cout<<"\n"<<a<<" - "<<b<<" = "<<c;  
    cout<<"\n Difference of "<<a<<" and "<<b<< " is: "<<c; 
    return 0;  
}  
